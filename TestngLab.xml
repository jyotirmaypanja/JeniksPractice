<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="User Suite" verbose="1">

  <test name="Sanity and Smoke Tests" parallel="methods" thread-count="2">
    <groups>
      <run>
        <include name="Sanity"/>
        <include name="Smoke"/>
        <exclude name="Regression"/>
      </run>
    </groups>

    <classes>
      <class name="com.wiproJuly.Testng.FirstTestng"/>
    </classes>
  </test>

</suite>
